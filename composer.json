{
    "name": "chilic/php.of.by",
    "license": "MIT",
    "description": "PHP UG site source code",
    "type": "project",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "owenversteeg/min",
                "version" : "dev-gh-pages",
                "source": {
                    "url": "https://github.com/owenversteeg/min",
                    "type": "git",
                    "reference": "gh-pages"
                }
            }
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-iconv": "*",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "ext-zip": "*",
        "easycorp/easyadmin-bundle": "1.*",
        "friendsofsymfony/ckeditor-bundle": "1.*",
        "friendsofsymfony/user-bundle": "2.1.*",
        "owenversteeg/min": "dev-gh-pages#fe73afc9e1a0ae3492ad3af2efeb0f4d47f967cc",
        "pear/archive_tar": "1.4.*",
        "pear/pear-core-minimal": "1.*",
        "sensio/framework-extra-bundle": "^5.1",
        "stof/doctrine-extensions-bundle": "1.3.*",
        "symfony/asset": "^4.1",
        "symfony/console": "^4.1",
        "symfony/expression-language": "^4.1",
        "symfony/flex": "^1.0",
        "symfony/form": "^4.1",
        "symfony/framework-bundle": "^4.1",
        "symfony/lts": "^4@dev",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "*",
        "symfony/process": "^4.1",
        "symfony/security-bundle": "^4.1",
        "symfony/serializer-pack": "*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/twig-bundle": "^4.1",
        "symfony/validator": "^4.1",
        "symfony/web-link": "^4.1",
        "symfony/webpack-encore-pack": "*",
        "symfony/yaml": "^4.1"
    },
    "require-dev": {
        "elao/web-profiler-extra-bundle": "2.*",
        "friendsofphp/php-cs-fixer": "2.*",
        "pdepend/pdepend": "2.*",
        "phpmd/phpmd": "2.*",
        "phpunit/phpunit": "7.*",
        "sebastian/phpcpd": "4.*",
        "sensiolabs/security-checker": "4.*",
        "squizlabs/php_codesniffer": "2.*",
        "symfony/debug-pack": "*",
        "symfony/dotenv": "^4.1",
        "symfony/maker-bundle": "^1.0",
        "symfony/profiler-pack": "*",
        "symfony/requirements-checker": "1.*",
        "symfony/test-pack": "^1.0",
        "symfony/web-server-bundle": "^4.1",
        "vimeo/psalm": "2.*"
    },
    "config": {
        "platform": {
            "php": "7.1.18"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "PhpOfBy\\": "src/PhpOfBy",
            "CommonBundle\\": "src/CommonBundle"
        },
        "classmap": [
            "src/Kernel.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script",
            "requirements-checker": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false
        }
    }
}
